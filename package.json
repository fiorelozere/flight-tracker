{
  "name": "flight-tracker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "ci": "ng lint && ng build",
    "watch": "ng build --watch --configuration development",
    "compodoc": "compodoc -p tsconfig.json",
    "format:test": "prettier --list-different \"./projects/**/*.{ts,html,scss,json}\"",
    "format:write": "prettier --write \"./projects/**/*.{ts,html,scss,json}\"",
    "analyze": "ng build --stats-json --output-hashing=none --named-chunks && esbuild-visualizer --template treemap --metadata dist/flight-tracker-dashboard/stats.json --filename dist/flight-tracker-dashboard/analyse/index.html  && http-server -o -c-1 ./dist/flight-tracker-dashboard/analyse/",
    "analyze:sme": "ng build --source-map --output-hashing=none --named-chunks && source-map-explorer dist/flight-tracker-dashboard/browser/*.js --html dist/flight-tracker-dashboard/sme/index.html && http-server -o -c-1 ./dist/flight-tracker-dashboard/sme/",
    "analyze:deps": "npm-run-all analyze:deps:*",
    "analyze:deps:all": "madge projects/flight-tracker-dashboard/src/main.ts --ts-config tsconfig.json --image ./deps/flight-tracker-dashboard/_all.png",
    "analyze:deps:ui": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/flight-tracker-dashboard/ui.png projects/flight-tracker-dashboard/src/app/ui/",
    "analyze:deps:pattern": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/flight-tracker-dashboard/pattern.png projects/flight-tracker-dashboard/src/app/pattern/",
    "analyze:deps:feature": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/flight-tracker-dashboard/feature.png projects/flight-tracker-dashboard/src/app/feature/",
    "analyze:deps:layout": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/flight-tracker-dashboard/layout.png projects/flight-tracker-dashboard/src/app/layout/",
    "analyze:deps:core": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/flight-tracker-dashboard/core.png projects/flight-tracker-dashboard/src/app/core/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.3",
    "@angular/cdk": "^18.2.3",
    "@angular/common": "^18.2.3",
    "@angular/compiler": "^18.2.3",
    "@angular/core": "^18.2.3",
    "@angular/forms": "^18.2.3",
    "@angular/material": "^18.2.3",
    "@angular/platform-browser": "^18.2.3",
    "@angular/platform-browser-dynamic": "^18.2.3",
    "@angular/router": "^18.2.3",
    "@ngrx/signals": "^18.0.2",
    "chart.js": "^4.4.3",
    "esbuild-visualizer": "^0.6.0",
    "http-server": "^14.1.1",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jwt-decode": "^4.0.0",
    "madge": "^8.0.0",
    "ng2-charts": "^6.0.1",
    "ngxtension": "^4.0.0",
    "npm": "^10.8.2",
    "prettier": "^3.3.3",
    "rxjs": "~7.8.0",
    "source-map-explorer": "^2.5.3",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.3",
    "@angular/cli": "^18.2.3",
    "@angular/compiler-cli": "^18.2.3",
    "@compodoc/compodoc": "^1.1.25",
    "@types/jasmine": "~5.1.0",
    "@types/jwt-decode": "^3.1.0",
    "@types/uuid": "^10.0.0",
    "angular-eslint": "18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-boundaries": "^4.2.2",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.1.0"
  }
}
